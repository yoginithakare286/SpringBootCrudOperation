            </column>
            <column name="project_information" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="project_manager" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="project_requirement" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="length_of_project" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="new_mold" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="transfer_mold" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="contact_review_date" type="date">
                <constraints nullable="true" />
            </column>
            <column name="created_by" type="varchar(255)">
                <constraints nullable="true" />
